				<dict>
					<key>source</key>
					<string>on run {input, parameters}		set share1 to "example-share-1" --change this	set share2 to "example-share-2" --change this	set share3 to "example-share-3" --change this		--set input to "ðŸ’¾âŒžIOP Fileserver â†’ -Jonathan â†’ test â†’ lots â†’ of â†’ folders â†’ Very Important File.mp4âŒŸðŸ’¾" --uncomment for testing		set input to input as string		set input to extractBetween(input, "âŒž", "âŒŸ")	set input to findAndReplaceInText(input, " â†’ ", ":")		if input starts with share1 &amp; ":" then		connectIfDisconnected(share1)	end if		if input starts with share2 &amp; ":" then		connectIfDisconnected(share2)	end if		if input starts with share3 &amp; ":" then		connectIfDisconnected(share3)	end if		try		return input as alias	on error		display alert ("Cannot find file") message ("This file may no longer exist. Please make sure the entire path was highlighted.")	end try	end runon connectIfDisconnected(server)	tell application "Finder" to set isConnected to disk server exists	if not isConnected then connectToServer(server)end connectIfDisconnectedon connectToServer(server)	set primaryServerAddress to "example.local" --change this	set secondaryServerAddress to "example.com" --change this		if addressExists(primaryServerAddress) then		tell application "Finder" to mount volume "smb://" &amp; primaryServerAddress &amp; "/" &amp; server	else if addressExists(secondaryServerAddress) then		tell application "Finder" to mount volume "smb://" &amp; secondaryServerAddress &amp; "/" &amp; server	else		display alert ("Unable to connect to " &amp; server) message ("Please connect to " &amp; server &amp; ", then try again.")		error number -128	end ifend connectToServeron addressExists(address)	try		do shell script "curl -I --connect-timeout 0.5 'http://" &amp; address &amp; "'" --will error if address does not exist		return true	on error		return false	end tryend addressExistson findAndReplaceInText(theText, theSearchString, theReplacementString)	set AppleScript's text item delimiters to theSearchString	set theTextItems to every text item of theText	set AppleScript's text item delimiters to theReplacementString	set theText to theTextItems as string	set AppleScript's text item delimiters to ""	return theTextend findAndReplaceInTextto extractBetween(SearchText, startText, endText)	set tid to AppleScript's text item delimiters -- save them for later.	set AppleScript's text item delimiters to startText -- find the first one.	set endItems to text of text item -1 of SearchText -- everything after the first.	set AppleScript's text item delimiters to endText -- find the end one.	set beginningToEnd to text of text item 1 of endItems -- get the first part.	set AppleScript's text item delimiters to tid -- back to original values.	return beginningToEnd -- pass back the piece.end extractBetween</string>
